tip <- function(p1, p0, lb, ub) {
if (lb > 1 & ub > 1){
b = lb
} else if (lb < 1 & ub < 1){
b = ub
} else stop("Please input a significant result.")
if(p1 >= 0 & p0 >= 0 & p1 <= 1 & p0 <= 1) stop("The prevalences entered must be between 0 and 1")
(1-p1+b*(p0-1))/(b*p0-p1)
}
tip <- function(p1, p0, lb, ub, explanation = FALSE) {
if (lb > 1 & ub > 1){
b = lb
} else if (lb < 1 & ub < 1){
b = ub
} else stop("Please input a significant result.")
if(p1 >= 0 & p0 >= 0 & p1 <= 1 & p0 <= 1) stop("The prevalences entered must be between 0 and 1")
gamma = (1-p1+b*(p0-1))/(b*p0-p1)
if (explanation = TRUE){
print(paste("An unmeasured confounder of size",gamma, "prevalent in",p1,"of the exposed population and", p0, "of the unexposed population would tip your (",lb,ub,") analysis to nonsignificance."))
} else gamma
}
