tip <- function(p1,p0 = 0,lb, ub) {
if (lb > 1 & ub > 1){
b = lb
} else if (lb < 1 & ub < 1){
b = ub
} else stop("Please input a significant result.")
(1-p1+b*(p0-1))/(b*p0-p1)
}
